var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];
(function() {
var gads = document.createElement('script');
gads.async = true;
gads.type = 'text/javascript';
var useSSL = 'https:' == document.location.protocol;
gads.src = (useSSL ? 'https:' : 'http:') + 
'//www.googletagservices.com/tag/js/gpt.js';
var node = document.getElementsByTagName('script')[0];
node.parentNode.insertBefore(gads, node);
})();
googletag.cmd.push(function() {
googletag.defineSlot('/1103991/美食天下内容页左幅300x250', [300, 250], 'div-gpt-ad-1415071902366-0').addService(googletag.pubads());
googletag.defineSlot('/1103991/美食天下内容页右幅300x250', [300, 250], 'div-gpt-ad-1415071902366-1').addService(googletag.pubads());
googletag.defineSlot('/1103991/美食天下菜谱频道右上角300x250', [300, 250], 'div-gpt-ad-1415071902366-2').addService(googletag.pubads());
googletag.defineSlot('/1103991/美食天下菜谱频道右侧二300x250', [300, 250], 'div-gpt-ad-1415071902366-3').addService(googletag.pubads());
googletag.defineSlot('/1103991/美食天下菜谱频道右侧(悬浮)300x250', [300, 250], 'div-gpt-ad-1415071902366-4').addService(googletag.pubads());
googletag.pubads().enableSingleRequest();
googletag.enableServices();
});