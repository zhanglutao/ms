$('.navlist_a').removeClass('on');
},200,200);

$("#navlist").click(function(){
if($('.navlist_a').hasClass("on")){
$("#navlist_sub").slideUp('2000');
$('.navlist_a').removeClass('on');
}
else{
$('.navlist_a').addClass('on');
$("#navlist_sub").slideDown('2000');
}
});